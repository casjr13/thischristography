{
  "name": "thischristography",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://thischristography.com",
  "dependencies": {
    "@types/lodash": "^4.14.123",
    "@types/react-router-dom": "^4.3.2",
    "bootstrap": "^4.3.1",
    "lodash": "^4.17.11",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-photo-gallery": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "yarn run build-css && node-sass src/ -o src/ --watch --recursive",
    "start": "react-scripts-ts start",
    "build": "yarn run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "deploy": "yarn build && gh-pages -d build -b master"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.12.0",
    "typescript": "^3.3.3333"
  }
}
